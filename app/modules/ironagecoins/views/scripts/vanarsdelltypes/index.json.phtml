<?php
$paginator = $this->va;
$results = array();
foreach($paginator as $result){
	$results[] = $result;
}

$meta = array(
    'currentPage' => $paginator->getCurrentPageNumber(),
    'totalResults' => $paginator->getTotalItemCount(),
    'resultsPerPage' => $paginator->getItemCountPerPage(),
    'totalPages' => abs($paginator->getTotalItemCount()/$paginator->getItemCountPerPage()));


echo Zend_Json::prettyPrint(Zend_Json::encode( ( array('meta' => $meta, 'vanArsdellTypes' => $results ) ) ) );
